{
  "name": "fitnesstrackr",
  "version": "1.0.0",
  "description": "an API for our new fitness empire, FitnessTrac.kr, using node, express, and postgresql",
  "main": "index.js",
  "scripts": {
    "seed:dev": "nodemon ./db/seed.js",
    "seed": "node ./db/seed.js",
    "start:dev": "nodemon index.js",
    "start": "node index.js",
    "heroku-postbuild": "npm run client:build",
    "test": "jest --runInBand",
    "test:watch:api": "jest --watchAll api",
    "test:watch": "jest --watchAll --runInBand",
    "test:watch:db": "jest --watchAll db",
    "client:build": "react-scripts build",
    "start:front": "react-scripts start",
    "deploy": "serve -s build"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "node",
    "express",
    "postgresql",
    "javascript"
  ],
  "author": "Fullstack Academy",
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.2",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}